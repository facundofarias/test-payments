{
	"name": "Test Payments",
	"description": "Right-click context menu to add payments test data",
	"version": "0.4",
	"permissions": ["contextMenus"],
	"background": {
		"scripts": ["extension.js"]
	},
	"content_scripts": [{
		"matches" : ["<all_urls>"],
		"js": ["context-element.js"],
		"run_at": "document_idle"
	}],
	"manifest_version": 2,
	"icons": {
		"128": "testpayments.png",
		"16": "testpayments-16.png"
	}
}
